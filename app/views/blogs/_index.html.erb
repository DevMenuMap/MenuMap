<% if current_user %>
	<div id="mm-blog">
		<div role="tabpanel">
			<ul class="nav nav-tabs" role="tablist">
				<li role="presentation" class="mm-blog-n-title active">
					<a href="#naver_blogs" role="tab" data-toggle="tab">
						<%= image_tag "naver_logo.png" %>
					</a>
				</li>
				<li role="presentation" class="mm-blog-d-title">
					<a href="#daum_blogs" role="tab" data-toggle="tab">
						<%= image_tag "daum_logo.png" %>
					</a>
				</li>
			</ul>

			<div class="tab-content">
				<div role="tabpanel" class="tab-pane active" id="naver_blogs">
					<% if !@naver_blogs.blank? %>
						<%= render partial: "/blogs/blog", locals: { blogs: @naver_blogs } %>
					<% else %>
						<p>nothings found</p>
					<% end %>
				</div>

				<div role="tabpanel" class="tab-pane" id="daum_blogs">
					<% if !@daum_blogs.blank? %>
						<%= render partial: "/blogs/blog", locals: { blogs: @daum_blogs } %>
					<% else %>
						<p>nothings found</p>
					<% end %>
				</div>
			</div>
		</div>
	</div>
<% end %>
